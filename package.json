{
  "name": "touchbar-plus",
  "main": "./lib/touchbar-plus",
  "version": "0.0.0",
  "description": "Better Atom touchbar support for Mac OSX",
  "keywords": [
    "touchbar",
    "mac",
    "osx",
    "atom"
  ],
  "repository": "https://github.com/atom/touchbar-plus",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "scripts": {
    "compile": "babel src/ --out-dir lib",
    "electron-rebuild": "electron-rebuild --version 3.1.10 --which-module canvas --force",
    "dev": "npm run compile -- --watch",
    "postinstall": "npm run electron-rebuild"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "async": "^2.6.1",
    "color-convert": "^2.0.0",
    "emojilib": "^2.4.0",
    "history": "^4.7.2",
    "lodash": "^4.17.15",
    "octicons": "0.0.0-882a0ed25",
    "react": "16.8.2",
    "react-router": "^4.3.1",
    "svg2img": "^0.5.0",
    "touchbar-electron-renderer": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "electron-rebuild": "^1.8.4",
    "eslint": "^5.3.0",
    "eslint-config-healthier": "^1.0.2"
  }
}
