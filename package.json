{
  "name": "touchbar-plus",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "version": "0.0.0",
  "description": "Better Atom touchbar support for Mac OSX",
  "keywords": [
    "touchbar",
    "mac",
    "osx",
    "atom"
  ],
  "repository": "https://github.com/atom/touchbar-plus",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "scripts": {
    "compile": "rollup -c",
    "compile:watch": "rollup -c --watch",
    "electron-rebuild": "electron-rebuild --version 3.1.10 --which-module canvas --force",
    "dev": "npm run compile -- --watch",
    "lint": "eslint src/**/*.ts",
    "var": "export PKG_CONFIG_PATH=\"${PKG_CONFIG_PATH}:/usr/local/opt/libffi/lib/pkgconfig\"",
    "rebuild": "npm run var &&  npm run electron-rebuild",
    "postinstall": "npm run var && npm run electron-rebuild"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "async": "^2.6.1",
    "color-convert": "^2.0.0",
    "emojilib": "^2.4.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "octicons": "0.0.0-882a0ed25",
    "react": "16.10.2",
    "react-router": "^4.3.1",
    "svg2img": "^0.6.1",
    "touchbar-electron-renderer": "^0.3.2"
  },
  "devDependencies": {
    "@types/atom": "^1.36.1",
    "@types/color-convert": "^1.9.0",
    "@types/history": "^4.7.3",
    "@types/lodash": "^4.14.141",
    "@types/react": "^16.8.22",
    "@types/react-router": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "electron-rebuild": "^1.8.4",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.1",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2",
    "rollup": "^1.23.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "semantic-release": "^15.13.24",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
